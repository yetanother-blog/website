  "author": "mail@yetanother.blog",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-docs": "^6.1.19",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^2.32.4",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-feed": "^2.13.0",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-mdx": "^1.10.0",
    "gatsby-plugin-offline": "^3.10.1",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-plugin-typescript": "^2.12.0",
    "gatsby-plugin-typography": "^2.12.0",
    "gatsby-remark-copy-linked-files": "^2.10.0",
    "gatsby-remark-images": "^3.11.0",
    "gatsby-remark-prismjs": "^3.13.0",
    "gatsby-remark-responsive-iframe": "^2.11.0",
    "gatsby-remark-slug": "^0.1.0",
    "gatsby-remark-smartypants": "^2.10.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-source-iubenda": "^1.0.2",
    "gatsby-transformer-sharp": "^2.12.0",
    "github-slugger-typescript": "^1.0.1",
    "gsap": "^3.6.0",
    "moment": "^2.29.1",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-typography": "^0.16.19",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5",
    "ts-node": "^9.1.1",
    "typeface-source-code-pro": "^1.1.13",
    "typeface-source-sans-pro": "^1.1.13",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-links": "^6.1.19",
    "@storybook/addons": "^6.1.20",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.19",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/mdx-js__react": "^1.5.3",
    "@types/moment": "^2.13.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.7",
    "@types/styled-system": "^5.1.10",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/typography": "^0.16.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-gatsby": "^0.12.1",
    "gatsby-plugin-ts-config": "^1.1.5",
    "jest": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "netlify-cli": "^3.8.5",
    "netlify-plugin-gatsby-cache": "^0.3.2",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5"
  },
  "homepage": "https://yetanother.blog",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yetanother-blog/website.git"
  },
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "test": "jest --watchAll",
    "test:ci": "jest --maxWorkers=50%",
    "tsc": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.[jt]sx?$": "<rootDir>/src/setupTests/jest-preprocess.ts",
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ]
  }
}
