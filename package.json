{
  "name": "yetanother.blog",
  "description": "Yet another blog build w/ gatsby, styled-components and mdx",
  "version": "0.1.0",
  "author": "mail@yetanother.blog",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.7",
    "@storybook/addon-docs": "^5.3.18",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.20.27",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.3.5",
    "gatsby-plugin-mdx": "^1.1.9",
    "gatsby-plugin-offline": "^3.1.4",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-sharp": "^2.5.6",
    "gatsby-plugin-styled-components": "^3.2.3",
    "gatsby-plugin-typescript": "^2.3.3",
    "gatsby-plugin-typography": "^2.4.3",
    "gatsby-remark-copy-linked-files": "^2.2.3",
    "gatsby-remark-images": "^3.2.5",
    "gatsby-remark-prismjs": "^3.4.4",
    "gatsby-remark-responsive-iframe": "^2.3.3",
    "gatsby-remark-smartypants": "^2.2.3",
    "gatsby-source-filesystem": "^2.2.4",
    "gatsby-transformer-sharp": "^2.4.6",
    "gsap": "^3.2.6",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-typography": "^0.16.19",
    "styled-components": "^5.1.0",
    "styled-system": "^5.1.5",
    "ts-node": "^8.9.0",
    "typeface-source-code-pro": "^1.1.3",
    "typeface-source-sans-pro": "^1.1.5",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.17",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@types/jest": "^25.2.1",
    "@types/mdx-js__react": "^1.5.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^5.1.0",
    "@types/styled-system": "^5.1.9",
    "@types/testing-library__jest-dom": "^5.0.3",
    "@types/typography": "^0.16.3",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "babel-preset-gatsby": "^0.3.1",
    "gatsby-plugin-ts-config": "^0.2.2",
    "graphql": "^15.0.0",
    "jest": "^25.4.0",
    "prettier": "^2.0.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3"
  },
  "homepage": "https://yetanother.blog",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yetanother-blog/website.git"
  },
  "scripts": {
    "build": "gatsby build",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "test": "jest --watchAll",
    "test:ci": "jest --maxWorkers=50%",
    "tsc": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.[jt]sx?$": "<rootDir>/src/setupTests/jest-preprocess.ts"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ]
  }
}
