    "@storybook/addon-docs": "^5.3.19",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.22.10",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-feed": "^2.5.3",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-mdx": "^1.2.12",
    "gatsby-plugin-offline": "^3.2.7",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-plugin-sitemap": "^2.4.3",
    "gatsby-plugin-styled-components": "^3.3.2",
    "gatsby-plugin-typescript": "^2.4.3",
    "gatsby-plugin-typography": "^2.5.2",
    "gatsby-remark-copy-linked-files": "^2.3.3",
    "gatsby-remark-images": "^3.3.8",
    "gatsby-remark-prismjs": "^3.5.2",
    "gatsby-remark-responsive-iframe": "^2.4.3",
    "gatsby-remark-slug": "^0.1.0",
    "gatsby-remark-smartypants": "^2.3.2",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-source-iubenda": "^1.0.2",
    "gatsby-transformer-sharp": "^2.5.3",
    "github-slugger-typescript": "^1.0.1",
    "gsap": "^3.2.6",
    "moment": "^2.26.0",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-typography": "^0.16.19",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5",
    "ts-node": "^8.10.1",
    "typeface-source-code-pro": "^1.1.3",
    "typeface-source-sans-pro": "^1.1.5",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.17",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.3",
    "@types/mdx-js__react": "^1.5.2",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/styled-components": "^5.1.0",
    "@types/styled-system": "^5.1.9",
    "@types/testing-library__jest-dom": "^5.7.0",
    "@types/typography": "^0.16.3",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-gatsby": "^0.4.6",
    "gatsby-plugin-ts-config": "^0.2.2",
    "graphql": "^15.0.0",
    "jest": "^26.0.1",
    "jest-svg-transformer": "^1.0.0",
    "netlify-cli": "^3.8.1",
    "netlify-plugin-gatsby-cache": "^0.2.1",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3"
  },
  "homepage": "https://yetanother.blog",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yetanother-blog/website.git"
  },
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "test": "jest --watchAll",
    "test:ci": "jest --maxWorkers=50%",
    "tsc": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.[jt]sx?$": "<rootDir>/src/setupTests/jest-preprocess.ts",
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ]
  }
}
