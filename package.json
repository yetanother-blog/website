{
  "name": "yetanother.blog",
  "description": "Yet another blog build w/ gatsby, styled-components and mdx",
  "version": "0.1.0",
  "author": "mail@yetanother.blog",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.0.0",
    "@mdx-js/react": "^1.0.0",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^3.1.2",
    "gatsby-plugin-feed": "^3.1.0",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-image": "^1.1.2",
    "gatsby-plugin-manifest": "^3.2.0",
    "gatsby-plugin-mdx": "^2.1.0",
    "gatsby-plugin-offline": "^4.2.0",
    "gatsby-plugin-react-helmet": "^4.1.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^3.1.2",
    "gatsby-plugin-sitemap": "^3.1.0",
    "gatsby-plugin-styled-components": "^4.1.0",
    "gatsby-plugin-typescript": "^3.1.0",
    "gatsby-plugin-typography": "^3.1.0",
    "gatsby-remark-images": "^4.1.1",
    "gatsby-remark-prismjs": "^4.2.0",
    "gatsby-remark-responsive-iframe": "^3.1.0",
    "gatsby-remark-slug": "^0.1.0",
    "gatsby-remark-smartypants": "^3.1.0",
    "gatsby-source-filesystem": "^3.1.0",
    "gatsby-source-iubenda": "^1.0.2",
    "gatsby-transformer-sharp": "^3.1.0",
    "gatsby-transformer-yaml": "^3.1.0",
    "graphql": "^15.5.0",
    "prism-themes": "^1.6.0",
    "prismjs": "^1.23.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-typography": "^0.16.19",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5",
    "typeface-source-code-pro": "^1.1.13",
    "typeface-source-sans-pro": "^1.1.13",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-docs": "^6.2.1",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addons": "^6.2.1",
    "@storybook/react": "^6.1.21",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.22",
    "@types/mdx-js__react": "^1.5.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.9",
    "@types/styled-system": "^5.1.10",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/typography": "^0.16.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-gatsby": "^1.1.0",
    "gatsby-plugin-ts-config": "^1.1.5",
    "jest": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "netlify-plugin-gatsby-cache": "^0.3.2",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.3"
  },
  "homepage": "https://yetanother.blog",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yetanother-blog/website.git"
  },
  "scripts": {
    "build": "gatsby build --log-pages",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "test": "jest --watchAll",
    "test:ci": "jest --maxWorkers=50%",
    "tsc": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.[jt]sx?$": "<rootDir>/src/setupTests/jest-preprocess.ts",
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ]
  }
}
