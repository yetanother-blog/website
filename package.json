{
  "name": "yetanother.blog",
  "description": "Yet another blog build w/ gatsby, styled-components and mdx",
  "version": "0.1.0",
  "author": "mail@yetanother.blog",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.7",
    "@mdx-js/react": "^1.5.7",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.20.6",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-manifest": "^2.3.2",
    "gatsby-plugin-mdx": "^1.1.3",
    "gatsby-plugin-offline": "^3.1.1",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-plugin-tslint": "^0.0.2",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-plugin-typography": "^2.4.1",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-images": "^3.2.1",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-remark-responsive-iframe": "^2.3.1",
    "gatsby-remark-smartypants": "^2.2.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-sharp": "^2.4.2",
    "gsap": "^3.2.6",
    "prismjs": "^1.19.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "styled-components": "^5.0.1",
    "ts-node": "^8.8.1",
    "typeface-source-code-pro": "^1.1.3",
    "typeface-source-sans-pro": "^1.1.5",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.1",
    "@graphql-codegen/typescript": "1.13.1",
    "@graphql-codegen/typescript-operations": "1.13.1",
    "@graphql-codegen/typescript-react-apollo": "1.13.1",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^5.0.1",
    "@types/testing-library__jest-dom": "^5.0.2",
    "@types/typography": "^0.16.3",
    "babel-jest": "^25.2.3",
    "babel-preset-gatsby": "^0.3.1",
    "graphql": "^14.6.0",
    "jest": "^25.2.3",
    "prettier": "^2.0.2",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.2.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3"
  },
  "homepage": "https://yetanother.blog",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yetanother-blog/website.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "tsc": "tsc --noEmit",
    "lint": "tslint --project .",
    "gql-codegen": "graphql-codegen --config codegen.yml"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.[jt]sx?$": "<rootDir>/src/setupTests/jest-preprocess.ts"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ]
  }
}
